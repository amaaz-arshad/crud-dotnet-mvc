@model EmployeeCRUD.Models.Bank

<h3>Bank Details</h3>
<hr />

<div>
    <dl class="dl-horizontal">
        <dt>
            Bank Name
        </dt>

        <dd>
            @Html.DisplayFor(model => model.bank_name)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "EditBank", new { id = Model.bank_id }, new { @class = "btn btn-primary" })
    @Html.ActionLink("Go back", "Index", null, new { @class = "btn btn-default" })
</p>
